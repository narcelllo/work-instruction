<div>
    <h1>Login</h1>
</div>

 <div>
    <%= form_for :session, url: login_path, method: :post do |f| %>

      <div>
        <%= f.label :email %>
        <%= f.email_field :email, placeholder: 'Email' %>
      </div>

      <div>
        <%= f.label :password %>
        <%= f.password_field :password, placeholder: 'Password' %>
      </div>

      <%= f.submit 'Singin'%>
    <% end %>
  </div>
</div>
